    "firebase": "^10.4.0",
    "firebase-admin": "^11.10.1",
    "stripe": "^13.5.0",
    "twilio": "^4.15.0",
    "nodemailer": "^6.9.4",
    "puppeteer": "^21.3.6",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2",
    "form-data": "^4.0.0",
    "ws": "^8.13.0",
    "node-rsa": "^1.1.1",
    "crypto-js": "^4.1.1",
    "qrcode": "^1.5.3",
    "barcode": "^0.1.4",
    "pdf-lib": "^1.17.1",
    "jimp": "^0.22.10",
    "canvas": "^2.11.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "node-gyp": "^9.4.0",
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-native": "^0.72.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/validator": "^13.7.8",
    "@types/moment": "^2.13.0",
    "@types/ws": "^8.5.5",
    "@types/crypto-js": "^4.1.2",
    "@types/qrcode": "^1.5.2",
    "@types/fluent-ffmpeg": "^2.1.24",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "file-loader": "^6.2.0",
    "url-loader": "^4.1.1",
    "html-webpack-plugin": "^5.5.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "prettier": "^3.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-native": "^12.2.2",
    "@testing-library/jest-dom": "^6.1.3",
    "jest-environment-jsdom": "^29.6.2",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.1",
    "jsdoc": "^4.0.2",
    "typedoc": "^0.25.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setup.js"
    ],
    "moduleNameMapping": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/__mocks__/fileMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/test/**/*"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-typescript",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      ],
      "import/prefer-default-export": "off",
      "no-console": "warn",
      "no-debugger": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitizen"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
