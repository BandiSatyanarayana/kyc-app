{
  "name": "bharat-kyc",
  "version": "1.0.0",
  "description": "Lightweight KYC App for Rural India - Digital onboarding solution with AI-powered guidance",
  "main": "src/mobile/BharatKYCSDK.js",
  "scripts": {
    "start": "node src/backend/server.js",
    "dev": "nodemon src/backend/server.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docs": "jsdoc -c jsdoc.json",
    "wireframes": "open docs/Wireframes/index.html"
  },
  "keywords": [
    "kyc",
    "digital-onboarding",
    "rural-india",
    "ai",
    "voice-interface",
    "offline-capable",
    "multilingual",
    "accessibility"
  ],
  "author": "Bharat KYC Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bharat-kyc/bharat-kyc-app.git"
  },
  "bugs": {
    "url": "https://github.com/bharat-kyc/bharat-kyc-app/issues"
  },
  "homepage": "https://bharatkyc.com",
  "dependencies": {
    "react": "^18.2.0",
